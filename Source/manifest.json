{
	"manifest_version": 2,
	"name": "Kids These Days",
	"version": "1.2",
	"description": "Replaces the text 'millenials' with 'kids these days'.",
	"icons": { "16": "icon16.png",
               "48": "icon48.png",
               "128": "icon128.png" },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

